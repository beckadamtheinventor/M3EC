				"SNOW_LAYER": "SNOW",
				"SOIL": "GROUND",
				"SOLID_ORGANIC": "PLANTS",
				"SPONGE": "SPONGE",
				"STONE": "ROCK",
				"STRUCTURE_VOID": "STRUCTURE_VOID",
				"TNT": "TNT",
				"UNDERWATER_PLANT": "VINE",
				"WATER": "WATER",
				"WOOD": "WOOD",
