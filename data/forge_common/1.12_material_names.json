				"AGGREGATE": "AIR",
				"AIR": "AIR",
				"AMETHYST": "ROCK",
				"BAMBOO_SAPLING": "PLANTS",
				"BAMBOO": "PLANTS",
				"BARRIER": "BARRIER",
				"BUBBLE_COLUMN": "WATER",
				"CACTUS": "CACTUS",
				"CAKE": "CAKE",
				"CARPET": "CARPET",
				"COBWEB": "WEB",
				"DECORATION": "PLANTS",
				"DENSE_ICE": "PACKED_ICE",
				"EGG": "DRAGON_EGG",
				"FIRE": "FIRE",
				"GLASS": "GLASS",
				"GOURD": "GOURD",
				"ICE": "ICE",
				"LAVA": "LAVA",
				"LEAVES": "LEAVES",
				"METAL": "IRON",
				"MOSS_BLOCK": "LEAVES",
				"NETHER_SHOOTS": "VINE",
				"NETHER_WOOD": "GROUND",
				"ORGANIC_PRODUCT": "LEAVES",
				"PISTON": "PISTON",
				"PLANT": "PLANTS",
				"PORTAL": "PORTAL",
				"POWDER_SNOW": "SNOW",
				"REDSTONE_LAMP": "REDSTONE_LIGHT",
				"REPAIR_STATION": "ANVIL",
				"REPLACEABLE_NETHER_PLANT": "VINE",
				"REPLACEABLE_PLANT": "VINE",
